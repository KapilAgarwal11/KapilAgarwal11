#include <stdio.h>
int main(){
   //int constant
    const int intVal = 10;
    //float constant
    const float floatVal = 6.99;
    //charconstant
    const char charVal = 'K';
    printf("Integer constant:%d \n", intVal ); 
    printf("Floating point constant: %.2f\n", floatVal ); 
    printf("Character constant: %c/n",charVal);
    return 0;
 }
